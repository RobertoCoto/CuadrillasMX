<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.fyg.cuadrillas.dao.ParametroNTx">
<resultMap type="Parametro" id="parametro">
<result column="valor" property="valor"/>
<result column="parametro" property="parametro"/>
</resultMap>
	
	<!-- Consulta Parametro-->
<select id="consultaParametro" parameterType="Parametro" resultMap="parametro">
SELECT valor,parametro FROM parametros WHERE parametro = #{parametro}
</select>
</mapper>